<% if params[:file] == "estatuto" %>
	<object data="<%= @documento.estatuto %>" style="width:100%; height: 600px"></object>	

<% elsif params[:file] == "tabela_valores" %>
	<object data="<%= @documento.tabela_valores %>" style="width:100%; height: 600px"></object>

<% elsif params[:file] == "regimento_interno" %>
	<object data="<%= @documento.regimento_interno %>" style="width:100%; height: 600px"></object>	

<% else %>
	<object data="<%= @documento.regulamento %>" style="width:100%; height: 600px"></object>	
<% end %>
